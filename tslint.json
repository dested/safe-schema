{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {
  },
  "rules": {
    "radix": false,
    "member-ordering": false,
    "no-namespace": false,
    "no-reference": false,
    "trailing-comma": [
      false,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "no-console": false,
    "interface-over-type-literal": false,
    "no-bitwise": false,
    "array-type": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "max-classes-per-file": false,
    "no-unused-expression": false,
    "arrow-parens": false,
    "interface-name": false,
    "forin": false,
    "member-access": [
      true,
      "no-public"
    ],
    "prettier": true,
    "ter-arrow-body-style": [
      false,
      "never"
    ]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
